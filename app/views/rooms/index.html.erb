<div class="row">
    <div class="col-3 bg-dark text-white">
        <h4>Users</h4>
        <% @users.each do |user| %>
            <p><%= user.email %><%= button_to '+', rooms_url(:user_id => user.id), method: :post %></p>
        <% end %>
        <h4>Rooms</h4>
        <% @rooms.each do |room| %>
            <p><%= link_to room.id, room_path(room.id) %></p>
        <% end %>

    </div>
    <div class="col-9">
        <h5>Выберите диалог и начните общаться</h5>

    </div>

</div>